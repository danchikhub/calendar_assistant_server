{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "eslint:recommended", 
    "plugin:@typescript-eslint/recommended", 
    "prettier"],
  "rules": {
    "no-duplicate-imports":"error",
    "default-case-last": "error",
    "no-var":"error",
    "no-console": "warn",
    "prettier/prettier": "error"
  },
  "env": {
    "node": true,
    "es2021": true
  }
}